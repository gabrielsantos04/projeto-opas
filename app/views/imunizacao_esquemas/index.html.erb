<div class="card">  
  <div class="card-content">
    <span class="card-title grey-text text-darken-4" onclick="toogle_div('#pesquisa')">
      Pesquisa
    </span>
    <div id="pesquisa" class="not-show">
      <%= search_form_for @q, url: request.env['PATH_INFO'] do |f|  %>
      <%# f.text_field :name_cont, placeholder: "Pesquisar..." %>
      <%= f.submit "Filtrar", class:"btn" %>
      <% end %>
    </div>
  </div>
</div>

<table class="card striped responsive-table">
  <thead>
    <tr>
            <th>Imunizacao imunobiologicos</th>
            <th>Dose</th>
            <th>Esquema</th>
            <th>Status</th>
            <th>Justificativa</th>
          </tr>
  </thead>

  <tbody>
    <% @imunizacao_esquemas.each do |imunizacao_esquema| %>
    <tr>
              
          <td><%= link_to imunizacao_esquema.imunizacao_imunobiologicos,imunizacao_esquema %></td>
        
              

          <td><%= imunizacao_esquema.dose %></td>
        
              

          <td><%= imunizacao_esquema.esquema %></td>
        
              

          <td><%= imunizacao_esquema.status %></td>
        
              

          <td><%= imunizacao_esquema.justificativa %></td>
        
          </tr>
    <% end %>
  </tbody>
</table>

<div class="card-panel">
  <%= paginate(@imunizacao_esquemas)  %>
</div>

<%= link_to '+ Cadastrar', new_imunizacao_esquema_path, class:"btn" %>
