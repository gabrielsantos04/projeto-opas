<div class="card">  
  <div class="card-content">
    <span class="card-title grey-text text-darken-4" onclick="toogle_div('#pesquisa')">
      Pesquisa
    </span>
    <div id="pesquisa" class="not-show">
      <%= search_form_for @q, url: request.env['PATH_INFO'] do |f|  %>
      <%# f.text_field :name_cont, placeholder: "Pesquisar..." %>
      <%= f.submit "Filtrar", class:"btn" %>
      <% end %>
    </div>
  </div>
</div>

<table class="card striped responsive-table">
  <thead>
    <tr>
            <th>Nome</th>
            <th>Tipo contato</th>
            <th>Suspeito</th>
            <th>Confirmado</th>
            <th>Notificacao</th>
          </tr>
  </thead>

  <tbody>
    <% @notificacao_contatos.each do |notificacao_contato| %>
    <tr>
              
          <td><%= link_to notificacao_contato.nome,notificacao_contato %></td>
        
              

          <td><%= notificacao_contato.tipo_contato %></td>
        
              

          <td><%= notificacao_contato.suspeito %></td>
        
              

          <td><%= notificacao_contato.confirmado %></td>
        
              

          <td><%= notificacao_contato.notificacao %></td>
        
          </tr>
    <% end %>
  </tbody>
</table>

<div class="card-panel">
  <%= paginate(@notificacao_contatos)  %>
</div>

<%= link_to '+ Cadastrar', new_notificacao_contato_path, class:"btn" %>
