<% avaliacao_sensitivas.each_with_index  do |avaliacao_sensitiva, index| %>
    <div class="row">
      <strong>Data Avaliação:</strong>
      <%= avaliacao_sensitiva.data %>
    </div>
    <div class="row">
      <% avaliacao_sensitiva.sensitiva_images.each do |s| %>
          <div class="col s6">
            <img src="<%= s.imagem.url %>" alt="" width="200px">
          </div>
      <% end %>
    </div>
    <div class="no-print">
    <tr >
      <td><%= link_to "Editar", edit_avaliacao_sensitiva_path(avaliacao_sensitiva), target:"_self"  %></td>
      <td><%= link_to 'Excluir', avaliacao_sensitiva, method: :delete, data: { confirm: 'Deseja realmente excluir?' } %></td>
    </tr>
    </div>
<% end %>