<div class="card">  
  <div class="card-content">
    <span class="card-title grey-text text-darken-4" onclick="toogle_div('#pesquisa')">
      Pesquisa
    </span>
    <div id="pesquisa" class="not-show">
      <%= search_form_for @q, url: request.env['PATH_INFO'] do |f|  %>
      <%# f.text_field :name_cont, placeholder: "Pesquisar..." %>
      <%= f.submit "Filtrar", class:"btn" %>
      <% end %>
    </div>
  </div>
</div>

<table class="card striped responsive-table">
  <thead>
    <tr>
            <th>Imunizacao esquema</th>
            <th>Data administracao</th>
            <th>Lote</th>
            <th>Validade</th>
            <th>Laboratorio</th>
            <th>Unidade administracao</th>
            <th>Via administracao</th>
            <th>Profissional</th>
            <th>Observacoes</th>
          </tr>
  </thead>

  <tbody>
    <% @imunizacao_encerramentos.each do |imunizacao_encerramento| %>
    <tr>
              
          <td><%= link_to imunizacao_encerramento.imunizacao_esquema,imunizacao_encerramento %></td>
        
              

          <td><%= imunizacao_encerramento.data_administracao %></td>
        
              

          <td><%= imunizacao_encerramento.lote %></td>
        
              

          <td><%= imunizacao_encerramento.validade %></td>
        
              

          <td><%= imunizacao_encerramento.laboratorio %></td>
        
              

          <td><%= imunizacao_encerramento.unidade_administracao %></td>
        
              

          <td><%= imunizacao_encerramento.via_administracao %></td>
        
              

          <td><%= imunizacao_encerramento.profissional %></td>
        
              

          <td><%= imunizacao_encerramento.observacoes %></td>
        
          </tr>
    <% end %>
  </tbody>
</table>

<div class="card-panel">
  <%= paginate(@imunizacao_encerramentos)  %>
</div>

<%= link_to '+ Cadastrar', new_imunizacao_encerramento_path, class:"btn" %>
