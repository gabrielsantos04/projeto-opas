<div class="card">  
  <div class="card-content">
    <span class="card-title grey-text text-darken-4" onclick="toogle_div('#pesquisa')">
      Pesquisa
    </span>
    <div id="pesquisa" class="not-show">
      <%= search_form_for @q, url: request.env['PATH_INFO'] do |f|  %>
      <%# f.text_field :name_cont, placeholder: "Pesquisar..." %>
      <%= f.submit "Filtrar", class:"btn" %>
      <% end %>
    </div>
  </div>
</div>

<table class="card striped responsive-table">
  <thead>
    <tr>
            <th>Data</th>
            <th>Suspeito</th>
            <th>Confirmado</th>
            <th>Notificacao contato</th>
          </tr>
  </thead>

  <tbody>
    <% @contato_avaliacaos.each do |contato_avaliacao| %>
    <tr>
              
          <td><%= link_to contato_avaliacao.data,contato_avaliacao %></td>
        
              

          <td><%= contato_avaliacao.suspeito %></td>
        
              

          <td><%= contato_avaliacao.confirmado %></td>
        
              

          <td><%= contato_avaliacao.notificacao_contato %></td>
        
          </tr>
    <% end %>
  </tbody>
</table>

<div class="card-panel">
  <%= paginate(@contato_avaliacaos)  %>
</div>

<%= link_to '+ Cadastrar', new_contato_avaliacao_path, class:"btn" %>
