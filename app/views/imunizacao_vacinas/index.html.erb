<div class="card">  
  <div class="card-content">
    <span class="card-title grey-text text-darken-4" onclick="toogle_div('#pesquisa')">
      Pesquisa
    </span>
    <div id="pesquisa" class="not-show">
      <%= search_form_for @q, url: request.env['PATH_INFO'] do |f|  %>
      <%= f.text_field :nome_cont, placeholder: "Pesquisar..." %>
      <%= f.submit "Filtrar", class:"btn" %>
      <% end %>
    </div>
  </div>
</div>

<table class="card striped responsive-table">
  <thead>
    <tr>
            <th>Nome</th>
            <th>Descricao</th>
          </tr>
  </thead>

  <tbody>
    <% @imunizacao_vacinas.each do |imunizacao_vacina| %>
    <tr>
              
          <td><%= link_to imunizacao_vacina.nome,imunizacao_vacina %></td>
        
              

          <td><%= imunizacao_vacina.descricao %></td>
        
          </tr>
    <% end %>
  </tbody>
</table>

<div class="card-panel">
  <%= paginate(@imunizacao_vacinas)  %>
</div>

<%= link_to '+ Cadastrar', new_imunizacao_vacina_path, class:"btn" %>
