<%= form_for(imunizacao_imunobiologico) do |f| %>
  <% if imunizacao_imunobiologico.errors.any? %>
    <div id="error_explanation" class="card-panel red white-text">
      <h5>Resolva os problemas a seguir antes de Salvar:</h5>

      <ul>
      <% imunizacao_imunobiologico.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
<div class="card-panel">
  
  <div class="input-field">

    <%= f.label :tipo %>
    <%= f.text_field :tipo %>
    
  </div>

  <div class="input-field">

    <%= f.label :nome_vacina %>
    <%= f.text_field :nome_vacina %>
    
  </div>

  <div class="input-field">
    <%= f.collection_select :imunizacao_vacina_id, ImunizacaoVacina.all, :id, :name, prompt: true  %>
    
  </div>

  <div class="input-field">
    <%= f.collection_select :imunizacao_solicitacao_id, ImunizacaoSolicitacao.all, :id, :name, prompt: true  %>
    
  </div>

  <div class="input-field">

    <%= f.label :status %>
    <%= f.text_field :status %>
    
  </div>

  <div class="input-field">

    <%= f.label :justificativa_indeferimento %>
    <%= f.text_area :justificativa_indeferimento %>
    
  </div>

</div>
  <div class="actions">
    <%= f.submit "Salvar", class:"btn" %>
  </div>
<% end %>
