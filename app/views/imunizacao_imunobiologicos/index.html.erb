<div class="card">  
  <div class="card-content">
    <span class="card-title grey-text text-darken-4" onclick="toogle_div('#pesquisa')">
      Pesquisa
    </span>
    <div id="pesquisa" class="not-show">
      <%= search_form_for @q, url: request.env['PATH_INFO'] do |f|  %>
      <%# f.text_field :name_cont, placeholder: "Pesquisar..." %>
      <%= f.submit "Filtrar", class:"btn" %>
      <% end %>
    </div>
  </div>
</div>

<table class="card striped responsive-table">
  <thead>
    <tr>
            <th>Tipo</th>
            <th>Nome vacina</th>
            <th>Imunizacao vacina</th>
            <th>Imunizacao solicitacao</th>
            <th>Status</th>
            <th>Justificativa indeferimento</th>
          </tr>
  </thead>

  <tbody>
    <% @imunizacao_imunobiologicos.each do |imunizacao_imunobiologico| %>
    <tr>
              
          <td><%= link_to imunizacao_imunobiologico.tipo,imunizacao_imunobiologico %></td>
        
              

          <td><%= imunizacao_imunobiologico.nome_vacina %></td>
        
              

          <td><%= imunizacao_imunobiologico.imunizacao_vacina %></td>
        
              

          <td><%= imunizacao_imunobiologico.imunizacao_solicitacao %></td>
        
              

          <td><%= imunizacao_imunobiologico.status %></td>
        
              

          <td><%= imunizacao_imunobiologico.justificativa_indeferimento %></td>
        
          </tr>
    <% end %>
  </tbody>
</table>

<div class="card-panel">
  <%= paginate(@imunizacao_imunobiologicos)  %>
</div>

<%= link_to '+ Cadastrar', new_imunizacao_imunobiologico_path, class:"btn" %>
