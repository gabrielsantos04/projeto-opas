    function toggle_sidebar() {
      var sidebar = $('#left-sidebar-nav');
      var main = $('#main');
      if (sidebar.is(":visible")) {
        sidebar.hide();
        main.css("padding-left","0px");
      }else{
        sidebar.show();
        main.css("padding-left","240px");
      };

    }

  </script>
  <style type="text/css">
    .chosen-single{
      background: none !important;
      background-color: rgba(255, 255, 255, 0.9) !important;
      width: 100% !important;
      padding: 5px !important;
      border: 1px solid #f2f2f2 !important;
      border-radius: 2px !important;
      height: 3rem !important;
      font-size: 14px !important;
    }
    .chosen-drop{
      border: 1px solid #f2f2f2 !important;
      border-radius: 2px !important;
    }
    .chosen-search{
      width: 95%
    }

    .esconder {
      padding:10px;
      background: none;
    }

    .esconder {
      -moz-transition: all linear 1s;
      -webkit-transition: all linear 1s;
      -o-transition: all linear 1s;
      transition: all linear 1s;
    }

    .esconder.ng-hide {
      opacity: 0;
    }

    .esconder.ng-show {
      opacity: 1;
    }

    .repetir-lista {
      border: 1px solid black;
      padding: 10px;
      margin-bottom: 5px;
    }

    /* ng-enter ng-enter-active ng-leave ng-move */

    .repetir-lista.ng-enter {
      -moz-transition: 2s linear all;
      -webkit-transition: 2s linear all;
      -o-transition: 2s linear all;
      transition: 2s linear all;
      opacity: 0;
      background:green;
    }

    .repetir-lista.ng-enter-active {
      opacity: 1;
      background:white;
    }
      @media print{
        .hide_print{
          display:none;
        }
      }
  </style>
  <script type="text/javascript">
    $(document).on('nested:fieldAdded', function(event){
      // this field was just inserted into your form
      var field = event.field;
      // it's a jQuery object already! Now you can find date input
      var dateField = field.find('.datetimepicker');
      var chosenField = field.find('.chosen-select');
      // and activate datepicker on it
      dateField.datetimepicker();
      chosenField.chosen();
      $('select').material_select();
      $('.select2').select2();
        $('.datepicker').pickadate({
            selectMonths: true, // Creates a dropdown to control month
            selectYears: 15, // Creates a dropdown of 15 years to control year,
            today: 'Hoje',
            clear: 'Limpar',
            close: 'Ok',
            closeOnSelect: true // Close upon selecting a date,
        });
    });

  </script>
