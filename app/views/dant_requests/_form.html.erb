<%= nested_form_for(dant_request) do |f| %>
    <% if dant_request.errors.any? %>
        <div id="error_explanation" class="card-panel red white-text">
          <h5>Resolva os problemas a seguir antes de Salvar:</h5>

          <ul>
            <% dant_request.errors.full_messages.each do |message| %>
                <li><%= message %></li>
            <% end %>
          </ul>
        </div>
    <% end %>
    <div class="card-panel">

      <div class="input-field">


        <%= f.select :mes, [["MARÇO" , 3],["JUNHO" , 6],["SETEMBRO" , 9],["DEZEMBRO" , 12]],prompt: true %>
        <%= f.label :mes, "MÊS" %>
      </div>

      <div class="input">
        <%= f.label :dant_responsavel_program_id, "Responsável pelo Programa" %>
        <%= f.select :dant_responsavel_program_id, @responsaveis, {prompt: true}, {class:"browser-default select2"}   %>


      </div>
      <% if current_user.administrador? %>
          <div class="input">
            <%= f.label :cidade_id, "Cidade" %>
            <%= f.select :cidade_id, @cidades, {prompt: true}, {class:"browser-default select2"}   %>

          </div>
      <% end %>
      <br>
      <div class="input-field">

        <%= f.label :qtd_hipertensos, DantQuestion.find_by_pergunta(1) %>
        <%= f.number_field :qtd_hipertensos , readonly: true%>

      </div>

      <div class="input-field">

        <%= f.label :atendimento_hipertensos, DantQuestion.find_by_pergunta(2) %>
        <%= f.number_field :atendimento_hipertensos %>

      </div>

      <div class="input-field">

        <%= f.label :qtd_obitos_hipertensos, DantQuestion.find_by_pergunta(3) %>
        <%= f.number_field :qtd_obitos_hipertensos , readonly: true%>

      </div>

      <div class="input-field">

        <%= f.label :qtd_diabeticos , DantQuestion.find_by_pergunta(4)%>
        <%= f.number_field :qtd_diabeticos , readonly: true%>

      </div>

      <div class="input-field">

        <%= f.label :atendimento_diabeticos, DantQuestion.find_by_pergunta(5) %>
        <%= f.number_field :atendimento_diabeticos %>

      </div>

      <div class="input-field">

        <%= f.label :qtd_obitos_diabeticos, DantQuestion.find_by_pergunta(6) %>
        <%= f.number_field :qtd_obitos_diabeticos , readonly: true%>

      </div>

      <div class="input-field">

        <%= f.label :qtd_diabeticos_hipertencos, DantQuestion.find_by_pergunta(7) %>
        <%= f.number_field :qtd_diabeticos_hipertencos , readonly: true%>

      </div>

      <div class="input-field">

        <%= f.label :atendimento_diabeticos_hipertensos, DantQuestion.find_by_pergunta(8) %>
        <%= f.number_field :atendimento_diabeticos_hipertensos %>

      </div>
      <br>
      <div class="input-field">

        <%= f.label :qtd_tratamento_hemodialise, DantQuestion.find_by_pergunta(9) %>
        <%= f.number_field :qtd_tratamento_hemodialise %>

      </div>

      <div class="input-field">

        <%= f.label :qtd_nph, DantQuestion.find_by_pergunta(10) %>
        <%= f.number_field :qtd_nph, readonly: true %>

      </div>

      <div class="input-field">

        <%= f.label :qtd_frascos_nph, DantQuestion.find_by_pergunta(11) %>
        <%= f.number_field :qtd_frascos_nph , readonly: true%>

      </div>

      <div class="input-field">

        <%= f.label :qtd_regular, DantQuestion.find_by_pergunta(12) %>
        <%= f.number_field :qtd_regular, readonly: true %>

      </div>

      <div class="input-field">

        <%= f.label :qtd_frascos_regular, DantQuestion.find_by_pergunta(13) %>
        <%= f.number_field :qtd_frascos_regular , readonly: true%>

      </div>

      <!-- <div class="input-field">

    <%#= f.label :qtd_analoga , DantQuestion.find_by_pergunta(14)%>
    <%#= f.number_field :qtd_analoga %>
    
  </div>

  <div class="input-field">

    <%#= f.label :qtd_frascos_analoga, DantQuestion.find_by_pergunta(15) %>
    <%#= f.number_field :qtd_frascos_analoga %>
    
  </div>-->

      <div class="input-field">

        <%= f.label :qtd_tabagista , DantQuestion.find_by_pergunta(14)%>
        <%= f.number_field :qtd_tabagista, readonly: true %>

      </div>

      <div class="input-field">

        <%= f.label :qtd_atendimento_tabagista, DantQuestion.find_by_pergunta(15) %>
        <%= f.number_field :qtd_atendimento_tabagista %>

      </div>

      <div class="input-field">

        <%= f.label :qtd_etilista, DantQuestion.find_by_pergunta(16) %>
        <%= f.number_field :qtd_etilista, readonly: true %>

      </div>

      <div class="input-field">

        <%= f.label :qtd_atendimento_etilista, DantQuestion.find_by_pergunta(17) %>
        <%= f.number_field :qtd_atendimento_etilista %>

      </div>

      <div class="input-field">

        <%= f.label :qtd_obesos , DantQuestion.find_by_pergunta(18)%>
        <%= f.number_field :qtd_obesos , readonly: true%>

      </div>

      <div class="input-field">

        <%= f.label :qtd_obesidade_1, DantQuestion.find_by_pergunta(19) %>
        <%= f.number_field :qtd_obesidade_1 , readonly: true%>

      </div>

      <div class="input-field">

        <%= f.label :qtd_obesidate_2, DantQuestion.find_by_pergunta(20) %>
        <%= f.number_field :qtd_obesidate_2 , readonly: true%>

      </div>

      <div class="input-field">

        <%= f.label :qtd_obesidade_3, DantQuestion.find_by_pergunta(21) %>
        <%= f.number_field :qtd_obesidade_3, readonly: true %>

      </div>

      <div class="input-field">

        <%= f.label :soma_participacoes, DantQuestion.find_by_pergunta(22) %>
        <%= f.number_field :soma_participacoes %>

      </div>






      <div class="card">
        <div class="card-content">
          <span class="card-title">Pacientes</span>
          <%= f.fields_for :dant_request_pacients do |esq| %>
              <div class="row">
                <div class="col s4 input">
                  <%= esq.label :dant_pacient_id, "Paciente" %> <br>
                  <% if esq.object.dant_pacient_id %>
                    <strong><%= DantPacient.find(esq.object.dant_pacient_id) %></strong>
                  <% end %>
                  <%= esq.hidden_field :dant_pacient_id%>
                  <%= esq.hidden_field :idade%>
                  <%= esq.hidden_field :hipertenso%>
                  <%= esq.hidden_field :diabetico%>
                  <%= esq.hidden_field :tabagista%>
                  <%= esq.hidden_field :etilista%>
                  <%= esq.hidden_field :obeso%>
                  <%= esq.hidden_field :sexo%>

                </div>
                <div class="col s2">
                  <%= esq.label :frascos_diarios, "QTD Diária" %>
                  <%= esq.number_field :frascos_diarios, step: 1, readonly: true %>
                </div>
                <div class="col s2">
                  <%= esq.label :frascos_mensais, "QTD Mensal" %>
                  <%= esq.number_field :frascos_mensais, step: 1, readonly: true %>
                </div>
                <div class="col s4">
                  <%#= esq.link_to_remove "Remover" %>
                </div>
              </div>
          <% end %>
        </div>
        <div class="card-action">
          <%#= f.link_to_add "Inserir Paciente", :dant_request_pacients %>
        </div>
      </div>
    </div>
    <div class="actions">
      <%= f.submit "Salvar", class:"btn" %>
    </div>
<% end %>
