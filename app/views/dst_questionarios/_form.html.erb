<%= form_for(dst_questionario) do |f| %>
  <% if dst_questionario.errors.any? %>
    <div id="error_explanation" class="card-panel red white-text">
      <h5>Resolva os problemas a seguir antes de Salvar:</h5>

      <ul>
      <% dst_questionario.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
<div class="card-panel">
  
  <div class="input-field">

    <%= f.label :titulo %>
    <%= f.text_field :titulo %>
    
  </div>

  <div class="input-field">

    <%= f.label :descricao %>
    <%= f.text_area :descricao %>
    
  </div>

  <div class="input-field">

    <%= f.check_box :ativo %>
    <%= f.label :ativo %>

  </div>
  <p>&nbsp;</p>

</div>
  <div class="actions">
    <%= f.submit "Salvar", class:"btn" %>
  </div>
<% end %>
