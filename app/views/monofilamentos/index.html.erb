<div class="card">
<h1 class="center-align">Monofilamentos</h1>
</div>

<table class="card striped responsive-table">
  <thead>
    <tr>
            <th>Nome</th>
            <th>Cor</th>
            <th>Interpretacao</th>
          </tr>
  </thead>

  <tbody>
    <% @monofilamentos.each do |monofilamento| %>
    <tr>
              
          <td><%= link_to monofilamento.nome,monofilamento %></td>
        
              

          <td><%= monofilamento.cor %></td>
        
              

          <td><%= monofilamento.interpretacao %></td>
        
          </tr>
    <% end %>
  </tbody>
</table>

<div class="card-panel">
  <%= paginate(@monofilamentos)  %>
</div>

<%= link_to '+ Cadastrar', new_monofilamento_path, class:"btn" %>
